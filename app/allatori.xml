<config>
    <input>
        <dir in="${classesRoot}" out="${classesRoot}-obfuscated" />
         Kotlin support
        <dir in="${kotlinRoot}" out="${kotlinRoot}-obfuscated" />
    </input>

    <classpath>
        <jar name="${androidJar}" />
        <jar name="${classpathJars}" />
    </classpath>

    <keep-names>

